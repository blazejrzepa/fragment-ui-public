{
  "name": "@fragment_ui/telemetry",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Telemetry and analytics for Fragment UI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js",
    "./server": "./dist/server.js",
      "./github-integration": "./dist/github-integration.js",
      "./roi-metrics": "./dist/roi-metrics.js",
      "./db/database": {
        "import": "./dist/db/database.js",
        "require": "./dist/db/database.js",
        "types": "./dist/db/database.d.ts"
      },
      "./db/schema": {
        "import": "./dist/db/schema.js",
        "require": "./dist/db/schema.js",
        "types": "./dist/db/schema.d.ts"
      }
  },
  "scripts": {
    "build": "node scripts/build.mjs",
    "dev": "tsc --watch"
  },
  "keywords": [
    "telemetry",
    "analytics",
    "tracking"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0"
  }
}
